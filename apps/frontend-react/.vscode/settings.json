{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "files.eol": "\n",
  "typescript.preferences.importModuleSpecifier": "shortest",
  "eslint.validate": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "graphql"
  ],
  "cSpell.words": ["Nhost"]
}
